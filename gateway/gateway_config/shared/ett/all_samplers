											<data>/opt/geneos/resources/toolkit-views/all_samplers/latencyRTT/slowcat2here.txt</data>
										</filename>
									</file>
								</protocolReader>
								<messageParser>
									<regex>
										<filters>
											<filter>
												<data>
													<regex>Receiving</regex>
												</data>
											</filter>
											<filter>
												<data>
													<regex>MSGTYPE=new</regex>
												</data>
											</filter>
											<filter>
												<data>
													<regex>ORDSTATUS=pendingnew</regex>
												</data>
											</filter>
										</filters>
										<tags>
											<tag name="currentid">
												<data>
													<regex>\[(.[^]]*)</regex>
												</data>
											</tag>
											<tag name="datetime">
												<data>
													<regex>(^.{23})</regex>
												</data>
											</tag>
											<tag name="description">
												<data>
													<regex>\|CLORDID=(.[^\|]*)</regex>
												</data>
											</tag>
										</tags>
										<attributes>
											<timestamp>
												<tags>
													<tag ref="datetime"/>
												</tags>
												<format>
													<data>%Y-%m-%d %H:%M:%S%f</data>
												</format>
											</timestamp>
