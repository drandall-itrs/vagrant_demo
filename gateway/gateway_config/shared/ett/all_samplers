					<block>
						<if>
							<gt>
								<dataItem>
									<property>@value</property>
								</dataItem>
								<integer>1</integer>
							</gt>
							<transaction>
								<update>
									<property>state/@severity</property>
									<severity>ok</severity>
								</update>
							</transaction>
							<if>
								<equal>
									<dataItem>
										<property>@value</property>
									</dataItem>
									<integer>1</integer>
								</equal>
								<transaction>
									<update>
										<property>state/@severity</property>
										<severity>warning</severity>
									</update>
								</transaction>
								<transaction>
									<update>
										<property>state/@severity</property>
										<severity>critical</severity>
									</update>
								</transaction>
							</if>
						</if>
					</block>
				</rule>
			</ruleGroup>
			<ruleGroup name="Service">
				<rule name="StatusHA">
					<targets>
						<target>/geneos/gateway/directory/probe/managedEntity/sampler[(@name=&quot;Coherence-Service&quot;)]/dataview/headlines/cell[(@name=&quot;StautsHA&quot;)]</target>
					</targets>
					<priority>1</priority>
					<block>
						<if>
							<equal>
								<dataItem>
									<property>@value</property>
								</dataItem>
								<string>MACHINE-SAFE</string>
							</equal>
							<transaction>
								<update>
									<property>state/@severity</property>
									<severity>ok</severity>
								</update>
							</transaction>
							<if>
								<equal>
									<dataItem>
										<property>@value</property>
									</dataItem>
									<string>NODE-SAFE</string>
								</equal>
								<transaction>
									<update>
										<property>state/@severity</property>
										<severity>warning</severity>
									</update>
								</transaction>
								<transaction>
									<update>
										<property>state/@severity</property>
										<severity>critical</severity>
									</update>
								</transaction>
							</if>
						</if>
					</block>
				</rule>
				<rule name="StorageEnabled">
					<targets>
						<target>/geneos/gateway/directory/probe/managedEntity/sampler[(@name=&quot;Coherence-Service&quot;)]/dataview/rows/row/cell[(@column=&quot;StorageEnabled&quot;)]</target>
					</targets>
					<priority>1</priority>
					<pathAliases>
						<pathAlias name="role">../cell[(@column=&quot;Role&quot;)]</pathAlias>
					</pathAliases>
					<block>
						<if>
							<and>
								<equal>
									<dataItem>
										<pathAlias ref="role"/>
										<property>@value</property>
									</dataItem>
									<string>cache</string>
								</equal>
								<equal>
									<dataItem>
										<property>@value</property>
									</dataItem>
									<string>true</string>
								</equal>
							</and>
							<transaction>
								<update>
									<property>state/@severity</property>
									<severity>ok</severity>
								</update>
							</transaction>
							<if>
								<and>
									<equal>
										<dataItem>
											<pathAlias ref="role"/>
											<property>@value</property>
										</dataItem>
										<string>cache</string>
									</equal>
									<notEqual>
										<dataItem>
											<property>@value</property>
										</dataItem>
										<string>true</string>
									</notEqual>
								</and>
								<transaction>
									<update>
										<property>state/@severity</property>
										<severity>critical</severity>
									</update>
								</transaction>
								<if>
									<equal>
										<dataItem>
											<property>@value</property>
										</dataItem>
										<string>true</string>
									</equal>
									<transaction>
										<update>
											<property>state/@severity</property>
											<severity>critical</severity>
										</update>
									</transaction>
									<transaction>
										<update>
											<property>state/@severity</property>
											<severity>ok</severity>
										</update>
									</transaction>
								</if>
							</if>
						</if>
					</block>
				</rule>
				<rule name="Service Counters">
					<targets>
						<target>/geneos/gateway/directory/probe/managedEntity/sampler/dataview[(@name=&quot;Coherence-Service&quot;)]/rows/row/cell[(@column=&quot;PartitionsUnbalanced&quot;)]</target>
						<target>/geneos/gateway/directory/probe/managedEntity/sampler/dataview[(@name=&quot;Coherence-Service&quot;)]/rows/row/cell[(@column=&quot;PartitionsVulnerable&quot;)]</target>
						<target>/geneos/gateway/directory/probe/managedEntity/sampler/dataview[(@name=&quot;Coherence-Service&quot;)]/rows/row/cell[(@column=&quot;ThreadAbandonedCount&quot;)]</target>
						<target>/geneos/gateway/directory/probe/managedEntity/sampler/dataview[(@name=&quot;Coherence-Service&quot;)]/rows/row/cell[(@column=&quot;TaskBacklog&quot;)]</target>
					</targets>
					<priority>1</priority>
					<block>
						<if>
							<gt>
								<dataItem>
									<property>@value</property>
								</dataItem>
								<integer>25</integer>
							</gt>
							<transaction>
								<update>
									<property>state/@severity</property>
									<severity>critical</severity>
								</update>
							</transaction>
							<if>
								<gt>
									<dataItem>
										<property>@value</property>
									</dataItem>
									<integer>5</integer>
								</gt>
								<transaction>
									<update>
										<property>state/@severity</property>
										<severity>warning</severity>
									</update>
								</transaction>
								<transaction>
									<update>
										<property>state/@severity</property>
										<severity>ok</severity>
									</update>
								</transaction>
							</if>
						</if>
					</block>
				</rule>
			</ruleGroup>
			<ruleGroup name="Logs">
				<rule name="Coherence logs">
					<targets>
						<target>/geneos/gateway/directory/probe/managedEntity[wild(@name,&quot;Node*&quot;)]/sampler/dataview[(@name=&quot;Coherence-Log Monitor&quot;)]/rows/row/cell[(@column=&quot;status&quot;)]</target>
					</targets>
					<priority>1</priority>
					<block>
						<if>
							<like>
								<dataItem>
									<property>@value</property>
								</dataItem>
								<string>OK</string>
							</like>
							<transaction>
								<update>
									<property>state/@severity</property>
									<severity>ok</severity>
								</update>
							</transaction>
							<transaction>
								<update>
									<property>state/@severity</property>
									<severity>warning</severity>
								</update>
							</transaction>
						</if>
					</block>
				</rule>
			</ruleGroup>
			<ruleGroup name="JMXThreads">
				<rule name="ThreadState">
