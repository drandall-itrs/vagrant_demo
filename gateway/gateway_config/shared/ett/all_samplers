				<targets>
					<target>/geneos/gateway/directory/probe/managedEntity/sampler[param(&quot;PluginName&quot;)=&quot;FKM&quot;][@type=&quot;trapmon&quot;]/dataview/rows/row[wild(@name,&quot;*#fail*&quot;)]/cell[@column=&quot;triggerDetails&quot;]</target>
