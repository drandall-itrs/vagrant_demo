		<ruleset name="API">
			<rule name="Queue Size">
				<targets>
					<target>/geneos/gateway[(@name=&quot;GATEWAY 2&quot;)]/directory/probe[(@name=&quot;192_168_31_135_7034&quot;)]/managedEntity[(@name=&quot;api&quot;)]/sampler[(@name=&quot;apiSampler&quot;)][(@type=&quot;api&quot;)]/dataview[(@name=&quot;jqueues&quot;)]/rows/row/cell[(@column=&quot;currentSize&quot;)]</target>
				</targets>
				<priority>1</priority>
				<block>
					<if>
						<gt>
							<dataItem>
								<property>@value</property>
							</dataItem>
							<integer>10000</integer>
						</gt>
						<transaction>
							<update>
								<property>state/@severity</property>
								<severity>critical</severity>
							</update>
						</transaction>
						<if>
							<gt>
								<dataItem>
									<property>@value</property>
								</dataItem>
								<integer>9000</integer>
							</gt>
							<transaction>
								<update>
									<property>state/@severity</property>
									<severity>warning</severity>
								</update>
							</transaction>
							<transaction>
								<update>
									<property>state/@severity</property>
									<severity>ok</severity>
								</update>
							</transaction>
						</if>
					</if>
				</block>
			</rule>
		</ruleset>
		<ruleset name="Arcontech">
			<rule name="reuters_feedstatus_value">
				<targets>
					<target>/geneos/gateway/directory/probe/managedEntity[(@name=&quot;arcontech&quot;)]/sampler[wild(@name,&quot;arcontech_reuters_*&quot;)][(@type=&quot;arcontech&quot;)]/dataview[wild(@name,&quot;arcontech_reuters_*&quot;)]/rows/row[(@name=&quot;FeedStatus&quot;)]/cell[(@column=&quot;value&quot;)]</target>
				</targets>
				<priority>1</priority>
				<block>
					<if>
						<equal>
							<dataItem>
								<property>@value</property>
							</dataItem>
							<string/>
						</equal>
						<transaction>
							<update>
								<property>state/@severity</property>
								<severity>undefined</severity>
							</update>
						</transaction>
					</if>
					<if>
						<like>
							<dataItem>
								<property>@value</property>
							</dataItem>
							<string>DOWN</string>
						</like>
						<transaction>
							<update>
								<property>state/@severity</property>
								<severity>critical</severity>
							</update>
						</transaction>
						<transaction>
							<update>
								<property>state/@severity</property>
								<severity>ok</severity>
							</update>
						</transaction>
					</if>
				</block>
			</rule>
			<rule name="server_state_value">
				<targets>
					<target>/geneos/gateway/directory/probe/managedEntity[(@name=&quot;arcontech&quot;)]/sampler[wild(@name,&quot;*_server&quot;)][(@type=&quot;arcontech&quot;)]/dataview[wild(@name,&quot;*_server&quot;)]/rows/row[(@name=&quot;State&quot;)]/cell[(@column=&quot;value&quot;)]</target>
				</targets>
				<priority>2</priority>
				<block>
					<if>
						<equal>
							<dataItem>
								<property>@value</property>
							</dataItem>
							<string/>
						</equal>
						<transaction>
							<update>
								<property>state/@severity</property>
								<severity>undefined</severity>
							</update>
						</transaction>
					</if>
					<if>
						<unlike>
							<dataItem>
								<property>@value</property>
							</dataItem>
							<string>UP</string>
						</unlike>
						<transaction>
							<update>
								<property>state/@severity</property>
								<severity>critical</severity>
							</update>
						</transaction>
						<transaction>
							<update>
								<property>state/@severity</property>
								<severity>ok</severity>
							</update>
						</transaction>
					</if>
				</block>
			</rule>
