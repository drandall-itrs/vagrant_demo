			<ruleGroup name="Node">
				<rule name="Available Memory">
					<targets>
						<target>/geneos/gateway/directory/probe/managedEntity/sampler[(@name=&quot;Node&quot;)][(@type=&quot;Coherence&quot;)]/dataview/rows/row/cell[(@column=&quot;AvailableMemory&quot;)]</target>
					</targets>
					<priority>1</priority>
					<pathAliases>
						<pathAlias name="role">parent::node()/cell[(@column=&quot;Role&quot;)]</pathAlias>
					</pathAliases>
					<block>
						<if>
							<and>
								<notEqual>
									<dataItem>
										<pathAlias ref="role"/>
										<property>@value</property>
									</dataItem>
									<string>CacheServer</string>
								</notEqual>
								<unlike>
									<dataItem>
										<pathAlias ref="role"/>
										<property>@value</property>
									</dataItem>
									<string>Proxy*</string>
								</unlike>
							</and>
							<transaction>
								<update>
									<property>state/@severity</property>
									<severity>undefined</severity>
								</update>
							</transaction>
							<if>
								<lte>
									<dataItem>
										<property>@value</property>
									</dataItem>
									<var ref="COHERENCE_NODE_AVAIL_MEMORY_FAIL"/>
								</lte>
								<transaction>
									<update>
										<property>state/@severity</property>
										<severity>critical</severity>
									</update>
								</transaction>
								<if>
									<lte>
										<dataItem>
											<property>@value</property>
										</dataItem>
										<var ref="COHERENCE_NODE_AVAIL_MEMORY_WARN"/>
									</lte>
									<transaction>
										<update>
											<property>state/@severity</property>
											<severity>warning</severity>
										</update>
									</transaction>
									<transaction>
										<update>
											<property>state/@severity</property>
											<severity>ok</severity>
										</update>
									</transaction>
								</if>
							</if>
						</if>
					</block>
				</rule>
