		<ruleset name="Compute Engine - Trading Environment">
			<rule name="UpAndDown">
				<targets>
					<target>/geneos/gateway/directory/probe/managedEntity/sampler/dataview[(@name=&quot;Components&quot;)]/rows/row/cell[(@column=&quot;Line1&quot;)]</target>
					<target>/geneos/gateway/directory/probe/managedEntity/sampler/dataview[(@name=&quot;Components&quot;)]/rows/row/cell[(@column=&quot;Line2&quot;)]</target>
					<target>/geneos/gateway/directory/probe/managedEntity/sampler/dataview[(@name=&quot;Components&quot;)]/rows/row/cell[(@column=&quot;PrimaryServer&quot;)]</target>
					<target>/geneos/gateway/directory/probe/managedEntity/sampler/dataview[(@name=&quot;Components&quot;)]/rows/row/cell[(@column=&quot;SecondaryServer&quot;)]</target>
				</targets>
				<priority>1</priority>
				<block>
					<if>
						<equal>
							<dataItem>
								<property>@value</property>
							</dataItem>
							<string>down</string>
						</equal>
						<transaction>
							<update>
								<property>state/@severity</property>
								<severity>critical</severity>
							</update>
						</transaction>
						<transaction>
							<update>
								<property>state/@severity</property>
								<severity>ok</severity>
							</update>
						</transaction>
					</if>
				</block>
			</rule>
