<?xml version="1.0" encoding="ISO-8859-1"?>
<netprobe
	compatibility="1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://schema.itrsgroup.com/GA2011.2-110303/netprobe.xsd"
>
	<selfAnnounce>
		<enabled>true</enabled>
		<retryInterval>60</retryInterval>
		<requireReverseConnection>true</requireReverseConnection>
		<probeName><hostname/><data>_INF_SA_</data><port/></probeName>
		<managedEntity>
			<name><hostname/><data>_INF_SA_</data><port/></name>
			<attributes>
				<attribute name="LICENCE MODEL">BASELINE AND FRAMEWORK</attribute>
				<attribute name="REGION">EMEA</attribute>
				<attribute name="DEPARTMENT">BACK OFFICE</attribute>
				<attribute name="PRODUCT">GENEOS</attribute>
				<attribute name="CITY">LONDON</attribute>
				<attribute name="APP">SELF ANNOUNCING NETPROBE</attribute>
			</attributes>
			<types>
				<type>baseline_san</type>
				
			</types>
		</managedEntity>
		<gateways>
			<gateway>
				<hostname>localhost</hostname>
				<port>7050</port>
			</gateway>
			<gateway>
				<hostname>localhost</hostname>
				<port>9999</port>
			</gateway>
		</gateways>
	</selfAnnounce>
</netprobe>
